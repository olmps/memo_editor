{
	"version": "2.0.0",
	"tasks": [
    {
      "type": "flutter",
      "command": "flutter",
      "runOptions": {
        "runOn": "folderOpen"
      },
      "args": [
        "pub",
        "run",
        "build_runner",
        "build"
      ],
      "problemMatcher": [
        "$dart-build_runner"
      ],
      "group": "build",
      "label": "Generate build_runner files"
    },
    {
      "type": "flutter",
      "command": "flutter",
      "args": [
        "pub",
        "run",
        "build_runner",
        "watch"
      ],
      "problemMatcher": [
        "$dart-build_runner"
      ],
      "group": "build",
      "label": "Watch build_runner files"
    }
  ]
}